
{

    "builds": [{

        "src": "djangoWebsite/wsgi.py",

        "use": "@vercel/python",

        "config": { "maxLambdaSize": "15mb", "runtime": "python3.11.4" }

    }],

    "routes": [

        {

            "src": "/(.*)",

            "dest": "djangoWebsite/wsgi.py"

        }

    ]

}